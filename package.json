{
  "name": "tween-animate",
  "version": "1.0.4",
  "description": "a tween animate library",
  "scripts": {
    "watch": "rollup -c --environment BUILD:development -w",
    "tsc": "tsc -d ./src/index.ts --declarationDir ./dist -emitDeclarationOnly",
    "build": "rollup -c --environment BUILD:production",
    "mocha": "mocha ./test/bundle/test.bundle.js",
    "template2": "hs -o template -p 8085 -c-1",
    "template": "supervisor -w ./dist/tween.umd.js ./template/server.js"
  },
  "keywords": [
    "tween",
    "aniamte"
  ],
  "main": "./dist/tween.umd.js",
  "author": "voderl",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-typescript": "^5.0.0",
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "rollup": "^2.18.1",
    "rollup-plugin-terser": "^6.1.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "eventemitter3": "^4.0.4",
    "express": "^4.17.1",
    "node-supervisor": "^1.0.2",
    "reload": "^3.1.0"
  }
}
